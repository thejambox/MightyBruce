draw_set_valign(fa_center);draw_set_halign(fa_center)
draw_text(xx,room_height*0.25,_heading)

if alarm[0] &gt; 0 exit
//selection
if instance_exists(obj_enterHS) exit
draw_set_color(global.OL);draw_set_font(f_hud)
var text;
    //set text
    text = _opt[_sel]
    //outline
    var ol = 2
    draw_text(xx+ol,room_height*_off[_sel],text)
    draw_text(xx-ol,room_height*_off[_sel],text)
    draw_text(xx,room_height*_off[_sel]+ol,text)
    draw_text(xx,room_height*_off[_sel]-ol,text)

//options
draw_set_color(c_white)

draw_text(xx,room_height*_off[0],_opt[0])//restart
draw_text(xx,room_height*_off[1],_opt[1])//exit


